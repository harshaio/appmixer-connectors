{
    "name": "appmixer.airshipcrm.core.SearchContacts",
    "author": "Harsha Surisetty <harsha@client.io>",
    "icon": "data:image/jpeg;base64,",
    "description": "Search for contacts by name, mobile number or email address in Airship.",
    "auth": {
        "service": "appmixer:airshipcrm"
    },
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "accountId": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "mobileNumber": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "oneOf": [
                    { "required": ["email"] },
                    { "required": ["mobileNumber"] },
                    { "required": ["name"] },
                    { "required": ["accountId"] }
                ]
            },
            "inspector": {
                "inputs": {
                    "accountId": {
                        "type": "text",
                        "index": 0,
                        "label": "Account Id",
                        "tooltip": "Airship account id."
                    },
                    "email": {
                        "type": "text",
                        "index": 1,
                        "label": "Email",
                        "tooltip": "Email address."
                    },
                    "mobileNumber": {
                        "type": "text",
                        "index": 2,
                        "label": "Mobile Number",
                        "tooltip": "Mobile number."
                    },
                    "name": {
                        "type": "text",
                        "index": 3,
                        "label": "Name",
                        "tooltip": "Name."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Result",
                    "value": "result"
                }
            ]
        }
    ]
}

